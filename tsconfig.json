{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./src", /* Base directory to resolve non-absolute module names. */
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}